{
  "name": "dydu-bliss",
  "version": "5.6.13",
  "private": true,
  "license": "MIT",
  "start_url": ".",
  "dependencies": {
    "@dydu_ai/oidc": "^1.1.8",
    "@mdi/js": "^4.8.95",
    "@mdi/react": "^1.3.0",
    "axios": "^0.19.2",
    "bowser": "^2.9.0",
    "classnames": "^2.2.6",
    "core-js": "^3.8.3",
    "css-loader": "^3.4.2",
    "debounce-promise": "^3.1.2",
    "i18next": "^19.8.3",
    "i18next-browser-languagedetector": "^4.0.1",
    "i18next-xhr-backend": "^3.2.2",
    "js-base64": "^3.7.2",
    "js-cookie": "^2.2.0",
    "jss-plugin-global": "^10.0.4",
    "jwt-decode": "^3.1.2",
    "keycloak-js": "^11.0.2",
    "qs": "^6.7.0",
    "react": "^17.0.0",
    "react-autosuggest": "^10.0.3",
    "react-dom": "^17.0.0",
    "react-i18next": "^11.3.3",
    "react-jss": "^10.0.4",
    "react-portal": "^4.2.1",
    "react-scripts": "^3.4.4",
    "react-swipeable": "^6.0.1",
    "socket.io-client": "^2.3.1",
    "string-replace-loader": "^3.0.1",
    "style-loader": "^1.3.0",
    "typeface-assistant": "0.0.75",
    "uuid4": "^1.1.4"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "dayjs": "^1.9.4",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^2.4.0",
    "git-revision-webpack-plugin": "^3.0.4",
    "husky": "^7.0.2",
    "jest": "^27.3.1",
    "lint-staged": "^10.5.1",
    "prettier": "^2.4.1",
    "react-styleguidist": "^10.6.2",
    "source-map-explorer": "^2.2.2",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2",
    "webpack-shell-plugin-next": "^1.2.0"
  },
  "scripts": {
    "build": "webpack --env prod --env onprem --config webpack.production.js",
    "buildCi": "webpack --env $BUILD_TARGET --env onprem --config webpack.production.js",
    "buildProd": "webpack --env prod --env sass  --config webpack.production.js",
    "buildPreprod": "webpack --env preprod --env sass --config webpack.production.js",
    "build:documentation": "styleguidist build --config .styleguide.config.js",
    "map": "source-map-explorer build/*bundle.min.js*",
    "start": "webpack-dev-server --config webpack.development.js",
    "start:documentation": "styleguidist server --config .styleguide.config.js",
    "format:all": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "test": "jest",
    "test:CI": "CI=true npm test",
    "prepare": "husky install",
    "pre-commit": "npm run test:CI && lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/public",
      "!<rootDir>/cookbook"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "text"
    ]
  }
}
