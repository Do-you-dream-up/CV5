{
  "application": {
    "open": 1,
    "qualification": true,
    "languages": ["en", "fr"],
    "defaultLanguage": ["fr"]
  },
  "avatar": {
    "background": false,
    "request": "",
    "response": "dydu.png"
  },
  "banner": {
    "active": true,
    "cookie": true,
    "dismissable": true,
    "more": true,
    "moreIcon": "open-in-new.png",
    "moreLink": "https://goo.gle",
    "transient": false
  },
  "carousel": {
    "bullets": true,
    "controls": true,
    "offset": 5,
    "width": 90
  },
  "chatbox": {
    "expandable": true,
    "margin": 16
  },
  "contacts": {
    "phone": true,
    "email": true,
    "socialNetwork": true
  },
  "dialog": {
    "top": true
  },
  "dragon": {
    "active": true,
    "boundaries": true,
    "factor": 100,
    "persist": true
  },
  "events": {
    "active": true,
    "features": {
      "chatbox": {
        "onMinimize": ["minimize"]
      },
      "teaser": {
        "onClick": ["foo", "bar"]
      }
    },
    "verbosity": 2
  },
  "feedback": {
    "active": true,
    "askChoices": true,
    "askComment": true
  },
  "font": {
    "url": ""
  },
  "footer": {
    "translate": true
  },
  "header": {
    "actions": {
      "close": true,
      "expand": true,
      "minimize": true,
      "more": true,
      "tests": false
    },
    "icons" : {
      "close": "close.png",
      "collapse": "arrow-collapse.png",
      "expand": "arrow-expand.png",
      "minimize": "window-minimize.png",
      "more": "cog.png"
    },
    "title": true
  },
  "input": {
    "counter": true,
    "delay": 300,
    "maxLength": 100
  },
  "interaction": {
    "avatar": {
      "request": false,
      "response": true
    },
    "loader": [400, 600]
  },
  "keycloak": {
    "enable": false,
    "realm": "front",
    "url": "https://keycloak.security.dydu-priv.com/auth/",
    "clientId": "v5"
  },
  "loader": {
    "size": 5
  },
  "menu": {
    "spacing": 4
  },
  "modal": {
    "maxWidth": "80%",
    "minWidth": "50%"
  },
  "onboarding": {
    "tips": true,
    "top": true
  },
  "root": "dydu-root",
  "secondary": {
    "automatic": true,
    "mode": "left",
    "width": 500,
    "transient": true
  },
  "spaces": {
    "active": true,
    "detection": [
      {
        "active": false,
        "mode": "cookie",
        "value": "dyduspace"
      },
      {
        "active": false,
        "mode": "localstorage",
        "value": "dyduspace"
      },
      {
        "active": false,
        "mode": "urlparameter",
        "value": "dyduspace"
      },
      {
        "active": false,
        "mode": "global",
        "value": "dyduspace"
      },
      {
        "active": false,
        "mode": "route",
        "value": {"/": "dydu"}
      },
      {
        "active": false,
        "mode": "hostname",
        "value": {"doyoudreamup.com": "dydu"}
      }
    ],
    "items": ["default", "foo", "bar"]
  },
  "suggestions": {
    "limit": 3
  },
  "tabs": {
    "items": [
      {"icon": "icons/message-text.png", "key": "dialog"},
      {"icon": "icons/account-box.png", "key": "contacts"}
    ],
    "selected": "dialog",
    "title": false
  },
  "teaser": {
    "bottom": 16,
    "right": 16
  },
  "top": {
    "size": 3
  },
  "Voice": {
    "enable": false,
    "ssml": "true",
    "sttServerUrl": "https://proxima.doyoudreamup.com:444",
    "ttsServerUrl": "https://proxima.doyoudreamup.com:444/tts",
    "voice": "Damien",
    "voiceSpace": "default"
  },
  "welcome": {
    "enable": true,
    "knowledgeName": "#welcome#"
  }
}
